{
  "compilerOptions": {
    "target": "ES2018",
    "module": "CommonJS",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "declarationMap": false,
    "preserveWatchOutput": true,
    "emitDeclarationOnly": false,
    "importHelpers": true,
    "strict": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "types": ["node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "incremental": true
  }
}
